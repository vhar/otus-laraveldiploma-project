use\040Illuminate\134Support\134Facades\134Storage;\012use\040Intervention\134Image\134Laravel\134Facades\134Image;\012use\040App\134Models\134Vezemkolesa\134Catalog\134Tyres\134Tyre;\012\012$tyre\040=\040Tyre::query()\012\040\040\040\040->with('productBrand')\012\040\040\040\040->with('productModel')\012\040\040\040\040->where('season',\040'Летние')\012\040\040\040\040->inRandomOrder()->first();\012\012$imges\040=\040$tyre->productModel->photos();\012\012$imgPath\040=\040parse_url($imges[0]->absolute_path,\040PHP_URL_PATH);\012$imgPath\040=\040ltrim($imgPath,\040sprint_f("/%s",\040env('AWS_BUCKET')));\012\012dump($imgPath);\012\012\012$storage\040=\040Storage::disk('s3');\012\012Image::read($storage->get("/"\040.\040$imgPath));\012\012/*\012if\040($tyreImage\040=\040Image::read(file_get_contents($imgUrl)))\040{\012\040\040\040\040$image\040=\040Image::read(file_get_contents(storage_path('tmp/bg.png')));\012\012\040\040\040\040$tyreImage->scale(width:\040844);\012\040\040\040\040$image->place($tyreImage,\040'top-left',\040352,\04095);\012\012\040\040\040\040$brandImage\040=\040Image::read(file_get_contents(str_replace("\040",\040"%20",\040$tyre->productBrand->logo()->absolute_path)));\012\040\040\040\040$brandImage->scale(width:\040292);\012\040\040\040\040$image->place($brandImage,\040'top-left',\04072,\04070);\012\012\040\040\040\040$image->save(storage_path("tmp/{$tyre->slug}.png"));\012}\012*/
